    "istanbul": "^0.4.5",
    "istanbul-combine": "^0.3.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-webpack": "^2.0.4",
    "style-loader": "^0.16.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^2.2.1",
